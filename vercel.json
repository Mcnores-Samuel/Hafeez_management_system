{
    "version": 2,
    "builds": [
      {
        "src": "cryptics_core_1/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.8"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "cryptics_core_1/wsgi.py"
      },
      {
        "src": "/(.*)",
        "dest": "cryptics_core_1/wsgi.py"
      }
    ],
    "env": {
      "PYTHONUNBUFFERED": "1",
      "DJANGO_SETTINGS_MODULE": "cryptics_core_1.settings" 
    }
}